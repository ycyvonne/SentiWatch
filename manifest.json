{
  "manifest_version": 2,
  "content_security_polTesticy": "script-src 'self' https://www.google.com; 'unsafe-eval'; object-src 'self'",
  "name": " SentiWatch",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["js/jquery-3.2.0.min.js", "content.js"]
  }],
 "browser_action": {
    "default_icon": "smiley_icon_purple.png",
    "default_popup": "popup.html"
  },
  "permissions": [

    "storage", "tabs"
  ],
  "web_accessible_resources": ["jquery-3.2.0.min.js", "inject.js"]
}
