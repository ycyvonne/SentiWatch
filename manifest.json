{
  "manifest_version": 2,
  "name": "Test Extension",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-3.2.0.min.js", "content.js"]
  }],
 "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": ["jquery-3.2.0.min.js", "inject.js"]
}